server {
	listen 80 default_server;
	server_name _;

	location / {
		uwsgi_pass localhost:5000; 
		include uwsgi_params;
	}

	location /prometheus {
		proxy_pass http://EmreKaratopuk-2:9090;
	}

	location /grafana {
		proxy_pass http://EmreKaratopuk-2:3000;
		proxy_set_header Host $http_host;
	}
}

server {
	listen 8080 default_server;

	location = /stub_status {
		stub_status;
	}
}